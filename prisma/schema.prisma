// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

//Models
model user {
  id Int @default(autoincrement()) @id
  email String
  password String
  userName String
  memberLevel Int
}